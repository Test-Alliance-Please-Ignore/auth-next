			"deleted_classes": ["UserTokenStore"]
		},
		{
			"tag": "v3",
			"new_sqlite_classes": ["UserTokenStore"]
		},
		{
			"tag": "v4",
			"new_sqlite_classes": ["CharacterDataStore"]
		}
	],
	"services": [
		{
			"binding": "SOCIAL_AUTH",
			"service": "social-auth"
		}
	],
	"vars": {
		"SENTRY_RELEASE": "unknown",
		"SENTRY_DSN": "https://68c0a0d3ba7849ea9f7af514744b4277@app.glitchtip.com/13245"
	}
}
